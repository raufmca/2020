package OOPS;

public class Overloading {

	public static void main(String[] args) {
		
		System.out.println("Addition of 2 integers -> "+add(10,20));
		System.out.println("Addition of float and integer -> " +add(100.0,25));
		System.out.println("Addtion of integer and float -> ");
		add(100, 450.0);
	}

	public static int add(int a, int b) {
		return a+b;
	}
	
	public static double add(double c, int z) {
		return c+z;
	}
	
	public static void add(int x, double y) {
		System.out.println("Inside no return function -> " + (x + y));
	}
}
