package interviewQuestions;

public class Overload {

	int x;
	double y;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		

	}
	
	 Overload() {
		this.x = 0;
		this.y = 0;
	}

	void add(int a , int b) {
		x = a+b;
	}
	
	void add(double c , double d) {
		y = c + d;
	}
	
}
